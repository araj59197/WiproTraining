        ["name=username", "name"],
        ["css=input[name='username']", "css:finder"]
      ],
      "value": ""
    }, {
      "id": "4",
      "comment": "",
      "command": "executeScript",
      "target": "let e=document.querySelector(\"input[name='username']\"); e.value='Admin'; e.dispatchEvent(new Event('input', {bubbles:true}));",
      "targets": [],
      "value": ""
    }, {
      "id": "5",
      "comment": "",
      "command": "click",
      "target": "name=password",
      "targets": [
        ["name=password", "name"],
        ["css=input[name='password']", "css:finder"]
      ],
      "value": ""
    }, {
      "id": "6",
      "comment": "",
      "command": "executeScript",
      "target": "let e=document.querySelector(\"input[name='password']\"); e.value='admin123'; e.dispatchEvent(new Event('input', {bubbles:true}));",
      "targets": [],
      "value": ""
    }, {
      "id": "7",
      "comment": "",
      "command": "click",
      "target": "xpath=//button[@type='submit']",
