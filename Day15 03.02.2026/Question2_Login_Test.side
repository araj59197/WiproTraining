      "value": ""
    }, {
      "id": "8",
      "comment": "",
      "command": "waitForElementPresent",
      "target": "xpath=//h6",
      "targets": [],
      "value": "10000"
    }, {
      "id": "9",
      "comment": "",
      "command": "assertText",
      "target": "xpath=//h6",
      "targets": [],
      "value": "Dashboard"
